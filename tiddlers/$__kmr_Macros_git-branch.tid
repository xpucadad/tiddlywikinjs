\define get-git-branch()
<$list filter="[[$:/temp/GitBranchInfo]get[branch]]" >
{{$:/temp/GitBranchInfo!!branch}}
</$list>
\end

\define display-git-branch(m)
<$reveal state="$:/temp/GitBranchInfo!!branch" type="nomatch" text="" >
$m$ @@color:red;<<get-git-branch>>@@
</$reveal>
\end