\define show-tabs(year)
<<tabs
  tabsList: "[suffix[AfibDataTable]has[year]!sort[]]"
  default: "$year$AfibDataTable"
  template: "$:/kmr/templates/AfibDataTableYearTemplate"
>>
\end

<$set name="year" value=<<now YYYY>> >
<$macrocall $name="show-tabs" year=<<year>> />
</$set>