<$list filter="[<detail-tiddler-name>!exists[]]">
<$action-createtiddler
  $basetitle=<<detail-tiddler-name>>
  $savetitle="!!month-details"
  text="{{||$:/kmr/templates/AfibMonthTemplate}}"
