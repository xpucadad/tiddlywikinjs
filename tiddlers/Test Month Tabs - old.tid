\define show-tabs(year, month)
year:$year$<br>
month:$month$<br>
{{{ [prefix[$year$-]suffix[AfibDataTable]!sort[]] }}}<br>

<<tabs
  tabsList: "[prefix[$year$-]suffix[AfibDataTable]!sort[]]"
  default: "$year$-$month$AfibDataTable"
  template: "$:/kmr/templates/AfibDataTableMonthTemplate"
  class: "tc-vertical"
>>
\end

<$set name="year" value=<<now YYYY>> >
year:<<year>><br>
<$set name="month" value=<<now 0MM>> >
<$macrocall $name="show-tabs" year=<<year>> month=<<month>> />
</$set>
</$set>
