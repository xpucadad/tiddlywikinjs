\define tabs-list(year)
[[$year$-08]] [[$year$-07]] [[$year$-06]] [[$year$-05]]
\end

\define show-tabs(year, tabs-list)
tabs-list in show-tabs: <<tabs-list>><br>
<<tabs
  tabsList: $tabs-list$
  default: "2021-07"
  template: "UFOMonthTemplate"
  class: tc-vertical
>>
\end


<$set name="year" value="2021">
year in main: <<year>><br>
<$set 
  name="tl" 
  value=<$macrocall $name="tabs-list" year=<<year>> /> 
>
tabs-list in main: <<tl>> <br>
<$macrocall $name="show-tabs" year=<<year>> tabs-list=<<tl>> />
</$set>
</$set>