\define tabs-list()
[[$(year)$-08]] [[$(year)$-07]] [[$(year)$-06]] [[$(year)$-05]]
\end

\define show-tabs(year, tabs-list)
<<tabs
  tabsList: "$tabs-list$"
  default: "2021-07"
  template: "UFOMonthTemplate"
  class: tc-vertical
>>
\end


<$set name="year" value="2021">
<$macrocall $name="show-tabs" year=<<year>> tabs-list=<<tabs-list>> />
</$set>